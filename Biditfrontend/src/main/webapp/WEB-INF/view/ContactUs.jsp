 <div class="container-fluid">
        <div class="row">
            <div class="col-md-4 col-sm-4 col-xs-12"></div>
            <div class="col-md-4 col-sm-4 col-xs-12">
                <form class="form-container" autocomplete="off">
                    <h1 style="text-align:center;">Contact Us</h1><br>
                    <div class="row">
                         <div class="form-group form-group-lg col-lg-12">
                            <input type="email" name="email" class="form-control" placeholder="E-mail ID">
                            <span class="glyphicon glyphicon-envelope"></span>
                        </div>
                    </div>
                    <div class="row">
                         <div class="form-group form-group-lg col-lg-12">
                            <input type="text" name="subject" class="form-control" placeholder="Subject">
                            <span class="glyphicon glyphicon-edit"></span>
                        </div>
                    </div>
                    <div class="row">
                        <div class="form-group form-group-lg col-lg-12">
                             <textarea rows="3" name="message" class="form-control" placeholder="Feedback"></textarea>
                        </div>
                    </div>
                    <div class="row">                        
                        <div class="form-group form-group-lg col-lg-12">     
                        <div class="btn-group-lg">
                            <button type="submit" name="send" class="btn btn-success btn-block">Send</button>
                        </div>
                        </div>
                    </div>
                </form>
            </div>
            <div class="col-md-4 col-sm-4 col-xs-12"></div>
        </div>
        </div>
   